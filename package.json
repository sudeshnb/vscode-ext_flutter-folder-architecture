{
  "name": "onyxsio",
  "displayName": "Onyxsio",
  "description": "Add new Features and Core easily with well-structured directories.",
  "version": "0.0.1",
  "publisher": "sudeshbandara",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sudeshnb/vscode-ext_flutter-folder-architecture.git"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Debuggers"
  ],
  "keywords": [
    "clean-architecture",
    "dart",
    "flutter",
    "folder",
    "state-management"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "onyxsio.feature",
        "title": "Create New Feature",
        "category": "Flutter Clean Architecture"
      },
      {
        "command": "onyxsio.core",
        "title": "Create New Core Folder",
        "category": "Flutter Clean Architecture"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "@types/lodash": "^4.14.194",
    "change-case": "^4.1.2",
    "mkdirp": "^3.0.1"
  }
}
